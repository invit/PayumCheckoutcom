{
    "name": "invit/payum-checkoutcom",
    "type": "library",
    "description": "Payum extension for integrating Checkout.com payments",
    "keywords": ["payment", "payum", "checkout.com"],
    "homepage": "https://www.weekend4two.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Alain Horner",
            "email": "alain.horner@weekend4two.com"
        },
        {
            "name": "Toni Rudolf",
            "email": "toni.rudolf@gmail.com"
        }
    ],
    "require": {
        "payum/core": "^1.3",
        "checkout/checkout-sdk-php": "^1.0"
    },
    "require-dev": {
        "payum/core": "^1.5@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Checkoutcom": "" }
    },
    "target-dir": "Payum/Checkoutcom",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
